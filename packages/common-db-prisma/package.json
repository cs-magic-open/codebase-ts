{
  "name": "@cs-magic/prisma",
  "packageManager": "yarn@4.1.1",
  "type": "module",
  "scripts": {
    "migrate-dev": "DATABASE_URL=$(sh load-env.sh) prisma migrate dev",
    "migrate-deploy": "DATABASE_URL=$(sh load-env.sh) prisma migrate deploy",
    "postinstall": "DATABASE_URL=$(sh load-env.sh) prisma generate",
    "db:push": "DATABASE_URL=$(sh load-env.sh) prisma db push",
    "db:studio": "DATABASE_URL=$(sh load-env.sh) prisma studio"
  },
  "dependencies": {
    "prisma": "^5.12.1",
    "prisma-json-types-generator": "^3.0.4"
  }
}
