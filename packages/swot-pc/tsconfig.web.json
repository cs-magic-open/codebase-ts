{
  "extends": [
    "@electron-toolkit/tsconfig/tsconfig.web.json",
    "../../tsconfig.dom.json",
  ],
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.tsx",
    "src/preload/*.d.ts"
  ],
  "compilerOptions": {
    "noUnusedLocals": false,
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ]
    }
  }
}
