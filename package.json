  "private": true,
  "workspaces": [
    "packages/*",
    "packages_frontend/*",
    "private_app-*/*",
    "app_*/*",
    "__tests__/*"
  ],
  "scripts": {
    "postinstall": "run-s patch db:generate build-wechaty",
    "patch": "ts-patch install -s && patch-package",
    "build-wechaty": "run-s .wechat4u .wechaty-puppet .wechaty-puppet-wechat4u .wechaty",
    ".wechat4u": "yarn workspace wechat4u compile",
    ".wechaty-puppet": "yarn workspace wechaty-puppet dist",
    ".wechaty-puppet-wechat4u": "yarn workspace wechaty-puppet-wechat4u dist",
    ".wechaty": "yarn workspace wechaty dist",
    "db:generate": "npx prisma generate",
    "db:migrate:dev": "npx prisma migrate dev",
    "db:migrate:deploy": "npx prisma migrate deploy",
    "test": "jest",
    "lint": "echo 'skip eslint'"
  },
  "resolutions": {
