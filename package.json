    "packages/*",
    "packages_*/*",
    "private_app-*/*",
    "app_*/*",
    "exts_*/*",
    "tests/*"
  ],
  "scripts": {
    "postinstall": "npx run-s db:generate install:tspc build:wechaty build:frontend",
    "db:generate": "npx prisma generate",
    "install:tspc": "npx ts-patch install -s",
    "build:frontend": "yarn workspace @cs-magic/react build",
    "build:wechaty": "npx run-s .wechat4u .wechaty-puppet .wechaty-puppet-wechat4u .wechaty-grpc .wechaty-puppet-service .wechaty",
    ".wechat4u": "yarn workspace wechat4u compile",
    ".wechaty-puppet": "yarn workspace wechaty-puppet dist",
    ".wechaty-puppet-wechat4u": "yarn workspace wechaty-puppet-wechat4u dist",
    ".wechaty-puppet-service": "yarn workspace wechaty-puppet-service dist",
    ".wechaty-grpc": "yarn workspace wechaty-grpc install:protoc && PATH=$PATH:$(go env GOPATH)/bin yarn workspace wechaty-grpc dist",
    ".wechaty": "yarn workspace wechaty dist"
  },
