{
  "name": "@cs-magic/swot-backend",
  "version": "0.1.0",
  "packageManager": "yarn@4.1.1",
  "type": "module",
  "main": "./dist/esm/src/index.js",
  "scripts": {
    "dev": "DOTENV_CONFIG_PATH=../../.env tsx -r dotenv/config src/main.ts | pino-pretty",
    "build": "rm -rf dist && tsc -b",
    "build:cjs": "tsc -b tsconfig.cjs.json && echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json",
    "start": "DOTENV_CONFIG_PATH=../../.env node -r dotenv/config dist/esm/src/main.js | pino-pretty",
    "stop": "",
    "postinstall": "yarn build"
  },
  "dependencies": {
    "@cs-magic/auth": "workspace:^",
    "@cs-magic/common": "workspace:^",
    "@cs-magic/env": "workspace:^",
    "@cs-magic/llm": "workspace:^",
    "@cs-magic/os": "workspace:^",
    "@cs-magic/spider": "workspace:^",
    "@cs-magic/swot-bot": "workspace:^",
    "@fastify/websocket": "^10.0.1",
    "@types/lodash": "^4.14.202",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "fastify-websocket": "^4.3.0",
    "lodash": "^4.17.21",
    "wechat4u": "workspace:^",
    "wechaty": "workspace:^",
    "wechaty-puppet": "workspace:^",
    "wechaty-puppet-padlocal": "workspace:^"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/ws": "^8.5.10",
    "typescript": "^5.4.5"
  }
}
