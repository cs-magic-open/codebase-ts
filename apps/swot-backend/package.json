{
  "name": "@cs-magic/swot-backend",
  "version": "0.1.0",
  "packageManager": "yarn@4.1.1",
  "type": "module",
  "scripts": {
    "dev": "DOTENV_CONFIG_PATH=../../.env tsx -r dotenv/config src/server.ts",
    "build": "rm -rf dist && tsc -b",
    "start": "node dist/src/index.js | pino-pretty",
    "stop": ""
  },
  "exports": {
    ".": "./dist/src/index.js",
    "./*": "./dist/src/*.js"
  },
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "dependencies": {
    "@cs-magic/common": "workspace:^",
    "@cs-magic/llm": "workspace:^",
    "@cs-magic/swot-bot": "workspace:^",
    "@fastify/websocket": "^10.0.1",
    "@types/lodash": "^4.14.202",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "fastify-websocket": "^4.3.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/ws": "^8.5.10",
    "typescript": "^5.4.5"
  }
}
